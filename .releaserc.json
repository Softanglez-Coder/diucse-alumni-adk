{
  "branches": [
    "main"
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {
            "type": "breaking change",
            "release": "major"
          },
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "test",
            "release": "patch"
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "type": "ci",
            "release": "patch"
          }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "projects/diucsealumni/core",
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist/diucsealumni/core",
        "npmPublish": true
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "projects/diucsealumni/models",
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist/diucsealumni/models",
        "npmPublish": true
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "projects/diucsealumni/services",
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist/diucsealumni/services",
        "npmPublish": true
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "projects/diucsealumni/utils",
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist/diucsealumni/utils",
        "npmPublish": true
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "package.json",
          "./projects/**/package.json"
        ]
      }
    ],
    "@semantic-release/github"
  ]
}
