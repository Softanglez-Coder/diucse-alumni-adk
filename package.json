{
  "name": "diucse-alumni-adk",
  "private": false,
  "workspaces": [
    "packages/*",
    "example-app"
  ],
  "version": "1.0.3-DEV.1",
  "description": "",
  "scripts": {
    "build:core": "npm run build --workspace=@diucsealumni/core",
    "build:utils": "npm run build --workspace=@diucsealumni/utils",
    "build:packages": "npm run build:core && npm run build:utils",
    "build:app": "npm run build --workspace=example-app",
    "build:all": "npm run build:packages && npm run build:app"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-static-copy": "^2.3.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main",
      {
        "name": "dev",
        "prerelease": "DEV"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "breaking change",
              "release": "major"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/core",
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/core",
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "packages/utils",
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/utils",
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "./packages/**/package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  }
}
